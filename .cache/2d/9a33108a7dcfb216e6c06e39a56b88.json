\n#teacher_details_container_3 div:nth-child(1) {\n    z-index: 3;\n    width: 25vw;\n    height: 17vw;\n    background-image: url(../img/people/rouka.jpeg);\n    order: 1;\n}\n\n#teacher_details_container_3 div:nth-child(2) {\n   order: 0;\n\n}\n\n#teacher_details_container_4 {\n    flex-direction: column;\n    align-items: center;\n    left: 53vw;\n    top: 0;\n}\n\n#teacher_details_container_4 div:nth-child(1) {\n    z-index: 1;\n    width: 21vw;\n    height: 26vw;\n    background-image: url(../img/people/topalidou.jpeg);\n}\n\n#teacher_details_container_5 {\n    flex-direction: column;\n    align-items: flex-end;\n    left: 71vw;\n    top:16vw;\n}\n\n#teacher_details_container_5 div:nth-child(1) {\n    z-index: 2;\n    width: 24vw;\n    height: 20vw;\n    background-image: url(../img/people/korfia.jpeg);\n}\n\n#teacher_details_container_6 {\n    flex-direction: column;\n    align-items: flex-end;\n    left: 45vw;\n    top: 37.5vw;\n}\n\n#teacher_details_container_6 div:nth-child(1) {\n    z-index: 2;\n    width: 35vw;\n    height: 18vw;\n    background-image: url(../img/people/stellatou.jpeg);\n}\n\n.teacher_name {\n    color: #c96b2c;\n    font-size: 1.1rem;\n    cursor: pointer;\n}\n\n.noscroll {\n    overflow-y: hidden;\n}\n\n.teacher_bio_container {\n    position: absolute;\n    left: 100vw;\n    top: 0;\n    display: none;\n    opacity: 0;\n    width: 100vw;\n    height: 100vh;\n    background-color: rgba(0, 0, 0, 0.4);\n    backdrop-filter: blur(40px);\n    z-index: 3;\n}\n\n.teacher_bio_header {\n    display: none;\n    opacity: 0;\n    position: absolute;\n    top: 10vh;\n    left: 70vw;\n    transform: translateX(-50%);\n    font-size: 3rem;\n    font-family: 'Hubballi', cursive;\n    color:#c96b2c;\n}\n\n.teacher_bio_details {\n    display: none;\n    opacity: 0;\n    position: absolute;\n    top: 20vh;\n    left: 70vw;\n    transform: translateX(-50%);\n    font-size: 1.4rem;\n    font-weight: 300;\n    text-align: justify;\n}"},"lineCount":null}},"er